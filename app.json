{
  "expo": {
    "name": "HolyQuran",
    "slug": "HolyQuran",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ptkzonelabs.holyquran"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "ptkzonelabs.holyquran"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
